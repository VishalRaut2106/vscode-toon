{
  "comments": {
    "lineComment": "#"
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"]
  ],
  "autoClosingPairs": [
    { "open": "{", "close": "}" },
    { "open": "[", "close": "]" },
    { "open": "\"", "close": "\"" }
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["\"", "\""]
  ],
  "folding": {
    "offSide": true
  },
  "indentationRules": {
    "increaseIndentPattern": "^\\s*.*:$",
    "decreaseIndentPattern": "^\\s*$"
  }
}
